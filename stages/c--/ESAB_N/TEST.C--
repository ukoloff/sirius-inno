long   i,Fint;
float x,y;
char  ch;
Fint:=fopen("iii.txt","w+t");
fseek(Fint,0);
for(i=0;i<10;i++)
{
	x:=i;y:=i+10;
  print("-x y ",x,y);
  read(ch);
  fprint(Fint,str(x,10,2), " ",str(y,10,2),"\n");
}
fclose(Fint);
Fint:=fopen("iii.txt","r+t");
fseek(Fint,0);
for(i=0;i<10;i++)
{
  fscan(Fint,x,y);
  print("-x y ",x,y);
  read(ch);
}
fclose(Fint);

